{
  "name": "vite-react-img",
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host 10.0.0.211 --port 3001",
    "build": "yarn lint && vite build",
    "lint": "yarn lint:pre && yarn lint:es && yarn lint:ts && yarn lint:style",
    "lint:es": "eslint --config .eslintrc.js ./src",
    "lint:fix": "yarn lint:pre --write && yarn lint:es --fix && yarn lint:style --fix",
    "lint:pre": "prettier -c .",
    "lint:style": "stylelint src/styles",
    "lint:ts": "tsc",
    "lint:watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "serve": "vite preview --host 10.0.0.211 --port 3001",
    "test": "jest",
    "test:nc": "jest --coverage=false -o -e",
    "test:watch": "yarn test:nc --watch",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "feather-icons": "^4.28.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-feather": "^2.0.9",
    "react-modal": "^3.14.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.0.0",
    "@types/feather-icons": "^4.7.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.3.7",
    "cssnano": "^5.0.8",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-badges": "^0.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-svg-transformer": "^1.0.0",
    "msw": "^0.35.0",
    "onchange": "^7.1.0",
    "pinst": "^2.0.0",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-plugin": "^1.0.0",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.42.1",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^2.0.0",
    "tailwindcss": "^2.2.17",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
